    package="in.lifc.customerapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CustomerApp"
        tools:targetApi="31">
        <activity
            android:name=".activity.EmiCalculator"
            android:exported="false" />
        <activity
            android:name=".activity.Aboutus"
            android:exported="false" />
        <activity
            android:name=".activity.Service_request"
            android:exported="false" />
        <activity
            android:name=".activity.Nearest_branch"
            android:exported="false" />
        <activity
            android:name=".activity.Loan_type"
            android:exported="false" />
        <activity
            android:name=".activity.NewLoanRequest"
